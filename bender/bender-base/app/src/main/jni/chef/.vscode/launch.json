{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "build and debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/chef",
        "args": ["starship_command_center_triangle.obj", "blach"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "clang++ build active file",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }